
.setFrame{
	background-color: #029400;
	text-align: center;
	font-size: 48rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	color: #ffffff;
	border-radius: 20rpx;
}
.framecontent{
	font-size: 48rpx !important;
	width: 96rpx !important;
}
.frame_view {
	width: 590rpx;
	height: 590rpx;
	padding: 20rpx;
	background: #792db3;
	z-index: 3;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	border-radius: 30rpx;
}
.img image{
	width: 42rpx;
	height: 42rpx;
}
.frame_row {
	width: 580rpx;
	height: 180rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.frame_item {
	width: 180rpx;
	height: 180rpx;
	z-index: 99;
}
