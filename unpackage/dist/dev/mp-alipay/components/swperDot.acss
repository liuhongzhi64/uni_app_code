
.dot-main {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.dot-item {
	width: 30rpx;
	height: 10rpx;
	border-radius: 3rpx;
	background-color: #c5c5c5;
	margin: 0 6rpx;
	box-sizing: border-box;
}
.active {
	border-radius: 5rpx;
	width: 30rpx;
	background-color: #fa3475;
}

