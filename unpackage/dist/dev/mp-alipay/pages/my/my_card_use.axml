<view class="my_card_use data-v-e3febc3e"><top-bar class="topBar data-v-e3febc3e" vue-id="320a1b1b-1" topBackgroundColor="{{topBackgroundColor}}" color="{{color}}" barImage="{{barImage}}" backImage="{{backImage}}" barName="{{barName}}" title="{{title}}" menuWidth="{{menuWidth}}" menuTop="{{menuTop}}" menuHeight="{{menuHeight}}" menuLeft="{{menuLeft}}" menuBottom="{{menuBottom}}" onVueInit="__l"></top-bar><view class="user-card-top data-v-e3febc3e" style="{{'top:'+(menuBottom+10+'px')+';'}}"><block a:for="{{topList}}" a:for-item="i" a:for-index="k" a:key="k"><view class="top-item data-v-e3febc3e"><block a:if="{{i.type==1}}"><view data-event-opts="{{[['tap',[['changeTopItem',[k,'$0'],[[['topList','',k,'type']]]]]]]}}" class="{{(('item-content item-all data-v-e3febc3e')+' '+((changeBtn==k)?'changeItem':''))}}" onTap="__e"><view class="item-all-name data-v-e3febc3e">{{i.name||classfiyName}}</view><view class="{{(('data-v-e3febc3e')+' '+(showClassify?'item-tall':'item-low'))}}"></view></view></block><block a:else><view class="{{(('item-content data-v-e3febc3e')+' '+((changeBtn==k)?'changeItem':''))}}"><view data-event-opts="{{[['tap',[['changeTopItem',[k,'$0'],[[['topList','',k,'type']]]]]]]}}" class="item-name data-v-e3febc3e" onTap="__e">{{i.name}}</view><block a:for="{{i.list}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['changSortType',[k,'$0',index],[[['topList','',k,'type']]]]]]]}}" class="tall-low data-v-e3febc3e" onTap="__e"><view class="{{(('item-tall data-v-e3febc3e')+' '+((item.sort_tall)?'changTallLow':''))}}"></view><view class="{{(('item-low data-v-e3febc3e')+' '+((item.sort_low)?'changTallLow':''))}}"></view></view></block></view></block></view></block></view><view class="{{(('classifyList data-v-e3febc3e')+' '+((showClassify)?'showClassify':''))}}" style="{{'top:'+(menuBottom+50+'px')+';'+('min-height:'+(height-menuBottom-50+'px')+';')}}"><view class="classifyList-content data-v-e3febc3e"><block a:for="{{classifyLists}}" a:for-item="item" a:for-index="k" a:key="k"><view data-event-opts="{{[['tap',[['changeCalssfiyItem',[k,'$0','$1'],[[['classifyLists','',k,'id']],[['classifyLists','',k,'name']]]]]]]}}" class="{{(('classify-item data-v-e3febc3e')+' '+((currentIndex==k&&item.isShow)?'changeItemClass':''))}}" onTap="__e">{{''+item.name+''}}</view></block></view><view class="define-item-btn data-v-e3febc3e"><view class="item-btn data-v-e3febc3e"><button class="define-btn data-v-e3febc3e" style="{{'background-color:'+(classfiyBtn!=-1?'#fa3475':'#999999')+';'}}" type="default" plain="true" disabled="{{itemBtn}}" data-event-opts="{{[['tap',[['define']]]]}}" onTap="__e">确定</button></view></view></view><view class="user-card-content data-v-e3febc3e"><scroll-view class="my_card-content data-v-e3febc3e" style="{{'padding-top:'+(menuBottom+50+'px')+';'}}"><view class="user-card-contents data-v-e3febc3e"><view class="card-use-hint data-v-e3febc3e">{{'卡券说明: '+card_intro+''}}</view><view class="user-card-porduct data-v-e3febc3e"><block a:for="{{porductList}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['gotoGoods',['$0'],[[['porductList','',index,'sku_id']]]]]]]}}" class="porduct-list data-v-e3febc3e" onTap="__e"><view class="porduct-items data-v-e3febc3e"><view class="porduct-item-images data-v-e3febc3e"><image src="{{requestUrl+item.head_img}}" mode class="data-v-e3febc3e"></image></view><view class="porduct-introduce data-v-e3febc3e"><view class="product-title data-v-e3febc3e">{{''+item.goods_name+''}}</view><block a:if="{{item.label}}"><view class="label data-v-e3febc3e"><block a:for="{{item.label.list}}" a:for-item="i" a:for-index="k" a:key="k"><view class="label-name data-v-e3febc3e">{{''+i+''}}</view></block></view></block><view class="porduct-price data-v-e3febc3e"><view class="porduct-original-cost data-v-e3febc3e"><text class="data-v-e3febc3e">￥</text>{{item.sale_price+''}}</view><block a:if="{{item.member_title}}"><view class="porduct