<view class="my_collection data-v-e97a0532"><top-bar class="topBar data-v-e97a0532" vue-id="45d99f56-1" topBackgroundColor="{{topBackgroundColor}}" color="{{color}}" backImage="{{backImage}}" barName="{{barName}}" title="{{title}}" menuWidth="{{menuWidth}}" menuTop="{{menuTop}}" menuHeight="{{menuHeight}}" menuLeft="{{menuLeft}}" menuBottom="{{menuBottom}}" onVueInit="__l"></top-bar><view class="top-swiper-tab data-v-e97a0532" style="{{'top:'+(menuBottom+10+'px')+';'}}"><swiper-tab-head vue-id="45d99f56-2" tabBars="{{tabBars}}" size="{{size}}" line="{{line}}" tabIndex="{{tabIndex}}" tabBackgroundColor="{{tabBackgroundColor}}" data-event-opts="{{[['^tabtap',[['tabtap']]]]}}" onTabtap="__e" class="data-v-e97a0532" onVueInit="__l"></swiper-tab-head></view><view class="my_collection-content data-v-e97a0532" style="{{'padding-top:'+(menuBottom+50+'px')+';'+('height:'+(height-menuBottom-50+'px')+';')}}"><scroll-view class="my_collection-content data-v-e97a0532"><view class="collection-content data-v-e97a0532"><block a:for="{{contentList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="content-list clearfix data-v-e97a0532"><view class="top-content data-v-e97a0532"><view class="head-image data-v-e97a0532"><image src="{{item.url}}" mode class="data-v-e97a0532"></image></view><view class="right-content data-v-e97a0532"><view class="goods_name data-v-e97a0532">{{item.goods_name}}</view><view class="label-list data-v-e97a0532"><block a:for="{{item.label}}" a:for-item="i" a:for-index="k" a:key="k"><view class="label data-v-e97a0532">{{''+i+''}}</view></block></view><view class="porduct-price-vip-cart data-v-e97a0532"><view class="porduct-price data-v-e97a0532"><text class="data-v-e97a0532">￥</text>{{item.price+''}}</view><block a:if="{{item.vipPrice>0}}"><view class="vip-cart-price data-v-e97a0532"><text class="vip-cart data-v-e97a0532">钻卡</text><text class="vip-price data-v-e97a0532">{{"￥"+item.vipPrice}}</text></view></block></view><view class="subscribe-goodReputation data-v-e97a0532"><view class="subscribe data-v-e97a0532">{{''+item.subscribe+'预约'}}</view><view class="goodReputation data-v-e97a0532">{{''+item.goodReputation+'%好评'}}</view></view></view></view><view class="delete-see-similarity data-v-e97a0532"><button class="delete data-v-e97a0532" type="default" plain="true" data-index="{{index}}" data-event-opts="{{[['tap',[['deleteItem',['$event']]]]]}}" onTap="__e">删除</button><button class="see-similarity data-v-e97a0532" type="default" plain="true">看相似</button></view></view></block></view></scroll-view></view></view>