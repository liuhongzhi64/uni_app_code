<view class="ticket-content data-v-ab11f3f2"><block s-for="{{ticketList}}" s-for-item="i" s-for-index="k" s-key="k"><view class="ticket-items data-v-ab11f3f2"><block s-if="{{i.expirationTime>0}}"><view class="ticket-number-expiration-time data-v-ab11f3f2"><view class="ticket-numer data-v-ab11f3f2">{{i.serialNumber}}</view><block s-if="{{i.state=='可使用'||i.state=='冻结中'||i.state=='已核销'}}"><view class="expiration-time data-v-ab11f3f2">{{i.expirationTime+"小时内过期"}}</view></block><block s-if="{{i.state=='已失效'||i.state=='已使用'}}"><view class="expiration-time data-v-ab11f3f2">删除</view></block></view></block><view class="ticket-items-content data-v-ab11f3f2"><view class="ticket-label-writer-state-userTime data-v-ab11f3f2"><view class="ticket-label-writer data-v-ab11f3f2"><text class="ticket-labels data-v-ab11f3f2" style="{{'background-image:'+(i.state=='可使用'||i.state=='冻结中'||i.receive>0&&i.state!='已结束'?'linear-gradient(-90deg,  '+i.goColor+' 0%,  '+i.toColor+' 100%)':' linear-gradient(-90deg,#999999 0%,  #999999 100%)')+';'}}">{{''+i.ticketLabel+''}}</text><text class="ticket-writer data-v-ab11f3f2">{{''+i.writer+''}}</text></view><block s-if="{{i.state&&i.state!='已结束'}}"><view class="ticket-state data-v-ab11f3f2">当前状态:<block s-if="{{i.state=='可使用'&&i.state!='冻结中'}}"><text style="{{'color:'+('#fa3475')+';'}}" class="data-v-ab11f3f2">{{''+i.state+''}}</text></block><block s-if="{{i.state!='可使用'&&i.state=='冻结中'}}"><text style="{{'color:'+('#0073c4')+';'}}" class="data-v-ab11f3f2">{{''+i.state+''}}</text></block><block s-if="{{i.state=='已使用'||i.state=='已失效'||i.state=='已核销'}}"><text style="{{'color:'+('#111111')+';'}}" class="data-v-ab11f3f2">{{i.state}}</text></block></view></block><block s-if="{{i.receive>=0&&i.receiveTime}}"><view class="can-receive data-v-ab11f3f2">{{'可领取'+i.receive+'张'}}</view></block><block s-if="{{i.userTime}}"><view class="user-time data-v-ab11f3f2">使用时间:<text class="data-v-ab11f3f2">{{i.userTime}}</text></view></block><block s-if="{{i.receiveTime&&i.allReceive>0}}"><view class="receive-time data-v-ab11f3f2">距结束还剩<text class="times data-v-ab11f3f2">23</text><text class="time-line data-v-ab11f3f2">:</text><text class="times data-v-ab11f3f2">23</text><text class="time-line data-v-ab11f3f2">:</text><text class="times data-v-ab11f3f2">23</text></view></block><block s-if="{{!i.receiveTime&&i.allReceive}}"><view class="receive-times data-v-ab11f3f2">已结束</view></block></view><block s-if="{{i.state}}"><view class="ticket-images-exclusiveName data-v-ab11f3f2" style="{{'background-image:'+(i.state=='可使用'||i.state=='冻结中'?'linear-gradient(-90deg,  '+i.goColor+' 0%,  '+i.toColor+' 100%)':' linear-gradient(-90deg,#999999 0%,  #999999 100%)')+';'}}"><view class="exclusive-name data-v-ab11f3f2">{{i.exclusiveName}}</view><view class="exclusive-price data-v-ab11f3f2"><text class="data-v-ab11f3f2">￥</text>{{''+i.exclusivePrice}}</view><view class="meet-price-user data-v-ab11f3f2">{{"满"+i.meetPriceUser+"元可用"}}</view><block s-if="{{i.state=='可使用'||i.state=='冻结中'||i.state=='已核销'}}"><view class="useing-ticket data-v-ab11f3f2" style="{{'color:'+(i.state=='已核销'?'#999999':i.toColor)+';'}}">立即使用</view></block><block s-if="{{i.receive>0}}"><view class="Immediately-receive useing-ticket data-v-ab11f3f2" style="{{'color:'+(i.state=='已结束'?'#999999':i.toColor)+';'}}">立即领取</view></block></view></block><block s-if="{{!i.state}}"><view class="ticket-images-exclusiveName data-v-ab11f3f2" style="{{'background-image:'+(i.receive>0&&i.receiveTime?'linear-gradient(-90deg,  '+i.goColor+' 0%,  '+i.toColor+' 100%)':' linear-gradient(-90deg,#999999 0%,  #999999 100%)')+';'}}"><view class="exclusive-name data-v-ab11f3f2">{{i.exclusiveName}}</view><view class="exclusive-price data-v-ab11f3f2"><text class="data-v-ab11f3f2">￥</text>{{''+i.exclusivePrice}}</view><view class="meet-price-user data-v-ab11f3f2">{{"满"+i.meetPriceUser+"元可用"}}</view><block s-if="{{i.receive>0}}"><view class="Immediately-receive useing-ticket data-v-ab11f3f2" style="{{'color:'+(i.receive>0&&i.receiveTime?i.toColor:'#999999')+';'}}">立即领取</view></block><block s-if="{{i.receive==0&&i.allReceive>=0}}"><view data-event-opts="{{[['tap',[['goUserCard',['$event']]]]]}}" class="useing-ticket data-v-ab11f3f2" style="{{'color:'+('#999999')+';'}}" bindtap="__e">立即使用</view></block></view></block></view><view data-event-opts="{{[['tap',[['showDetails',['$0'],[[['ticketList','',k,'serialNumber']]]]]]]}}" class="ticketDetails data-v-ab11f3f2" bindtap="__e"><view class="details-title data-v-ab11f3f2"><text class="data-v-ab11f3f2">卡券详情</text><image src="{{i.arrowImages}}" mode class="data-v-ab11f3f2"></image></view><block s-if="{{i.showTicketDetails}}"><view class="details-content data-v-ab11f3f2"><block s-for="{{i.ticketDetails}}" s-for-item="i" s-for-index="k"><view class="item-details data-v-ab11f3f2">{{i}}</view></block></view></block></view><block s-if="{{i.state=='已使用'}}"><view class="ticket-label-images data-v-ab11f3f2"><image src="../static/images/state2.png" mode class="data-v-ab11f3f2"></image></view></block><block s-if="{{i.state=='已失效'}}"><view class="ticket-label-images data-v-ab11f3f2"><image src="../static/images/state1.png" mode class="data-v-ab11f3f2"></image></view></block><block s-if="{{i.state=='已核销'}}"><view class="ticket-label-images data-v-ab11f3f2" style="top:80rpx;"><image src="../static/images/check.png" mode class="data-v-ab11f3f2"></image></view></block><block s-if="{{i.receive==0&&i.receiveTime&&i.uesrReceive==2}}"><view class="ticket-label-images data-v-ab11f3f2"><image src="../static/images/upper-limit.png" mode class="data-v-ab11f3f2"></image></view></block><block s-if="{{i.receive==0&&i.allReceive==0}}"><view class="ticket-label-images data-v-ab11f3f2"><image src="../static/images/loot-all.png" mode class="data-v-ab11f3f2"></image></view></block><block s-if="{{i.receive>0&&!i.receiveTime&&i.allReceive>0}}"><view class="ticket-label-images data-v-ab11f3f2"><image src="../static/images/ticke-over.png" mode class="data-v-ab11f3f2"></image></view></block></view></block><block s-for="{{cardsList}}" s-for-item="i" s-for-index="k" s-key="k"><view class="ticket-items data-v-ab11f3f2">1</view></block></view>