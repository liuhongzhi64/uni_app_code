<view class="diary data-v-61fd5b0e"><view class="diary-top-bar data-v-61fd5b0e"><view class="bar-name data-v-61fd5b0e" style="{{'height:'+(menuHeight+'px')+';'+('margin-top:'+(menuTop+'px')+';')+('border-radius:'+(menuHeight/2+'px')+';')+('line-height:'+(menuHeight+'px')+';')+('padding-right:'+(menuWidth+20+'px')+';')+('padding-bottom:'+(10+'px')+';')}}"><block tt:for="{{navigationList}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view data-event-opts="{{[['tap',[['change',[index]]]]]}}" class="{{(('top-navigation-bars data-v-61fd5b0e')+' '+((btnnum==index)?'checked':''))}}" bindtap="__e">{{''+item+''}}<view class="{{(('data-v-61fd5b0e')+' '+((btnnum==index)?'checkedBar':''))}}"></view></view></block></view></view><view class="content-all-bar data-v-61fd5b0e" style="{{'top:'+(menuBottom+10+'px')+';'}}"><view class="content-bar data-v-61fd5b0e"><scroll-view class="content-bar-items data-v-61fd5b0e" scroll-x="true"><view class="top-content data-v-61fd5b0e"><block tt:for="{{topTabList}}" tt:for-item="i" tt:for-index="k" tt:key="k"><view data-event-opts="{{[['tap',[['changeContentBar',[k]]]]]}}" class="top-content-list data-v-61fd5b0e" bindtap="__e"><view class="list-items data-v-61fd5b0e"><view class="item-image data-v-61fd5b0e"><image src="{{requestUrl+i.icon}}" mode class="data-v-61fd5b0e"></image></view><view class="item-name data-v-61fd5b0e">{{i.name}}</view></view></view></block></view></scroll-view></view></view><view class="content data-v-61fd5b0e" style="{{'padding-top:'+(menuBottom+85+'px')+';'+('min-height:'+(height-menuBottom-130+'px')+';')}}"><view class="content-list data-v-61fd5b0e"><scroll-view scroll-y="{{true}}" class="data-v-61fd5b0e"><view class="detail-content data-v-61fd5b0e"><view class="subject-content clearfix data-v-61fd5b0e"><block tt:for="{{contentList}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view class="{{(('subject-content-list data-v-61fd5b0e')+' '+(index%2==0?'item-fl':'item-fr'))}}"><view class="diary-images data-v-61fd5b0e"><image class="diary-image data-v-61fd5b0e" src="{{requestUrl+item.cover_img}}" mode></image></view><view class="label data-v-61fd5b0e">{{item.label}}</view><view class="diary-title data-v-61fd5b0e">{{''+item.title+''}}</view><block tt:if="{{item.doctor_name}}"><view class="doctor_name data-v-61fd5b0e">{{''+item.doctor_name+''}}</view></block><view class="goods_name data-v-61fd5b0e">{{item.goods_name}}</view><view class="head_ico-nick_name-collect_num data-v-61fd5b0e"><view class="head_ico-nick_name data-v-61fd5b0e"><image class="head_ico data-v-61fd5b0e" src="{{requestUrl+item.head_ico}}" mode></image><text class="nick_name data-v-61fd5b0e">{{item.nick_name}}</text></view><block tt:if="{{item.collect_num}}"><view class="collect_num data-v-61fd5b0e">{{''+item.collect_num+''}}</view></block><block tt:else><view class="collect_num data-v-61fd5b0e">0</view></block></view></view></block></view></view></scroll-view></view></view></view>