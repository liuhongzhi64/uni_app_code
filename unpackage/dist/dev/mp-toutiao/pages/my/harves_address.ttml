<view class="harves_address data-v-2ce88c3a"><top-bar class="topBar data-v-2ce88c3a" vue-id="70280feb-1" topBackgroundColor="{{topBackgroundColor}}" color="{{color}}" backImage="{{backImage}}" barName="{{barName}}" title="{{title}}" menuWidth="{{menuWidth}}" menuTop="{{menuTop}}" menuHeight="{{menuHeight}}" menuLeft="{{menuLeft}}" menuBottom="{{menuBottom}}" bind:__l="__l"></top-bar><view class="harves_address_content container data-v-2ce88c3a" style="{{'padding-top:'+(menuBottom+10+'px')+';'+('height:'+(height-menuBottom-10+'px')+';')}}"><block tt:if="{{list.length==0}}"><view class="usermottoNo data-v-2ce88c3a"><image src="https://img-blog.csdnimg.cn/20200720152049639.png" class="data-v-2ce88c3a"></image><text class="user-motto data-v-2ce88c3a">您还没有填写地址喔</text></view></block><block tt:else><view class="usermotto data-v-2ce88c3a"><block tt:for="{{list}}" tt:for-item="item" tt:for-index="index" tt:key="index"><block class="data-v-2ce88c3a"><view class="addressbox data-v-2ce88c3a"><view data-event-opts="{{[['tap',[['set_address',['$0'],[[['list','',index]]]]]]]}}" class="addressLine data-v-2ce88c3a" bindtap="__e"><view class="name data-v-2ce88c3a">{{item.accept_name}}</view><text class="phone data-v-2ce88c3a">{{item.telphone}}</text><view class="label data-v-2ce88c3a">{{item.tag}}</view><view class="address data-v-2ce88c3a"><text class="detailed data-v-2ce88c3a">{{item.province_cn+item.city_cn+item.area_cn+item.address}}</text></view></view><view class="editBox data-v-2ce88c3a"><view data-event-opts="{{[['tap',[['setDefault',['$0','$1'],[[['list','',index,'is_default']],[['list','',index,'id']]]]]]]}}" class="set data-v-2ce88c3a" bindtap="__e"><view class="{{(('circular data-v-2ce88c3a')+' '+((item.is_default==1)?'is_default':''))}}"></view>默认地址</view><view data-event-opts="{{[['tap',[['edit',['$0','$1'],[[['list','',index,'id']],[['list','',index]]]]]]]}}" class="edit data-v-2ce88c3a" bindtap="__e">编辑</view><view data-event-opts="{{[['tap',[['deleteArea',['$0'],[[['list','',index,'id']]]]]]]}}" class="delete data-v-2ce88c3a" bindtap="__e">删除</view></view></view></block></block></view></block><view class="address-btn data-v-2ce88c3a"><view data-event-opts="{{[['tap',[['bindViewTap',['$event']]]]]}}" class="btn data-v-2ce88c3a" bindtap="__e">+添加新地址</view></view></view></view>